{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "jsRules": {},
  "rules": {
    "semicolon": [true, "never"],
    "ordered-imports": [false],
    "no-console": false,
    "quotemark": [true, "single", "jsx-double"],
    "trailing-comma": [
      true,
      {
        "multiline": {
          "objects": "always",
          "arrays": "always",
          "exports": "always",
          "imports": "always",
          "functions": "never",
          "typeLiterals": "ignore"
        },
        "esSpecCompliant": true
      }
    ],
    "object-literal-sort-keys": [false],
    "variable-name": [false],
    "jsx-boolean-value": [false],
    "jsx-no-lambda": [false],
    "member-access": false
  },
  "rulesDirectory": []
}
