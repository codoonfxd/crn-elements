{
  "name": "@codoonfxd/crn-elements",
  "version": "0.6.2",
  "description": "A ReactNative Component Library of CodoonFXD.",
  "keywords": [
    "ReactNative",
    "Component"
  ],
  "author": "沈经纬 <327538014@qq.com>",
  "license": "MIT",
  "main": "node_modules/expo/AppEntry.js",
  "directories": {
    "dist": "dist",
    "test": "__tests__"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/codoonfxd/crn-elements"
  },
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "lint": "tslint -c tslint.json 'src/**/*.ts{,x}'",
    "docs-dev": "docsify serve docs",
    "changelog": "conventional-changelog -n ./node_modules/@codoonfxd/changelog-config/index.js -i docs/CHANGELOG.md -s -t -r 0 -p angular",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "run-ios": "react-native run-ios",
    "ios": "expo start --ios",
    "android": "expo start --android",
    "run-android": "react-native run-android",
    "pretty-ts": "prettier-tslint fix 'src/**/*.ts{,x}'"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "@codoonfxd/changelog-config": "^0.2.0",
    "@codoonfxd/commitlint-config": "^0.1.0",
    "@commitlint/cli": "^8.2.0",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^23.3.14",
    "@types/react": "^16.8.8",
    "@types/react-native": "^0.46.0",
    "@types/react-test-renderer": "^16.8.1",
    "babel-preset-expo": "^5.0.0",
    "conventional-changelog-cli": "^2.0.23",
    "docsify-cli": "^4.3.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "expo": "^32.0.0",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "jest-enzyme": "^7.0.2",
    "jest-expo": "32.0.0",
    "jest-fetch-mock": "^2.1.1",
    "jest-react-native": "^18.0.0",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^13.1.0",
    "metro-react-native-babel-preset": "^0.53.1",
    "prettier": "^1.16.4",
    "prettier-tslint": "^0.4.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-native": "https://github.com/expo/react-native/archive/sdk-32.0.0.tar.gz",
    "react-native-typescript-transformer": "^1.2.12",
    "react-navigation": "^3.9.1",
    "react-test-renderer": "^16.8.4",
    "schedule": "^0.4.0",
    "sinon": "^7.3.2",
    "ts-jest": "^23.10.5",
    "tslint": "^5.15.0",
    "typescript": "^3.6.3",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@codoonfxd/commitlint-config"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier-tslint fix",
      "npm run lint",
      "git add"
    ]
  },
  "dependencies": {
    "@types/color": "^3.0.0",
    "@types/lodash": "^4.14.141",
    "color": "^3.1.2",
    "lodash": "^4.17.15"
  },
  "jest": {
    "preset": "react-native",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js",
      "^.+\\.tsx?$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.jest.json"
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "modulePaths": [
      "<rootDir>"
    ],
    "setupFiles": [
      "./tests/setup.js"
    ]
  }
}
